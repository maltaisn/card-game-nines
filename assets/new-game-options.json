{
  "name": "io.github.maltaisn.cardgametest.new-game-settings",
  "prefs": {
    "player_count": {
      "type": "slider",
      "title": "@string/pref_player_count",
      "minValue": 2,
      "maxValue": 6,
      "defaultValue": 4
    },
    "difficulty": {
      "type": "slider",
      "title": "@string/pref_difficulty",
      "help": "@string/pref_difficulty_help",
      "enumValues": [
        "@string/pref_difficulty_0",
        "@string/pref_difficulty_1",
        "@string/pref_difficulty_2",
        "@string/pref_difficulty_3"
      ],
      "minValue": 0,
      "maxValue": 3,
      "defaultValue": 1
    },
    "game_speed": {
      "type": "slider",
      "title": "@string/pref_game_speed",
      "enumValues": [
        "@string/pref_game_speed_0",
        "@string/pref_game_speed_1",
        "@string/pref_game_speed_2",
        "@string/pref_game_speed_3",
        "@string/pref_game_speed_4"
      ],
      "minValue": 0,
      "maxValue": 4,
      "defaultValue": 1
    },
    "cheat_mode": {
      "type": "switch",
      "title": "@string/pref_cheat_mode",
      "help": "@string/pref_cheat_mode_help",
      "defaultValue": false
    }
  }
}