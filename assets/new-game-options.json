{
  name: "com.maltaisn.cardgametest.newGameSettings"
  prefs: {
    player_count: {
      type: "slider"
      title: "@string/pref_player_count"
      minValue: 2
      maxValue: 6
      defaultValue: 4
    }
    difficulty: {
      type: "slider"
      title: "@string/pref_difficulty"
      help: "@string/pref_difficulty_help"
      enumValues: [
        "@string/pref_difficulty_0"
        "@string/pref_difficulty_1"
        "@string/pref_difficulty_2"
        "@string/pref_difficulty_3"
      ]
      minValue: 0
      maxValue: 3
      defaultValue: 1
    }
    game_speed: {
      type: "list"
      title: "@string/pref_game_speed"
      entries: {
        very_slow: "@string/pref_game_speed_0"
        slow: "@string/pref_game_speed_1"
        normal: "@string/pref_game_speed_2"
        fast: "@string/pref_game_speed_3"
        very_fast: "@string/pref_game_speed_4"
      }
      defaultValue: "normal"
    }
    cheat_mode: {
      type: "switch"
      title: "@string/pref_cheat_mode"
      help: "@string/pref_cheat_mode_help"
      defaultValue: false
    }
  }
}